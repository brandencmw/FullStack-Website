        data-bs-ride="carousel"
        data-bs-interval="false"
      >
        <!-- Indicator buttons -->
        <div class="carousel-indicators">
          <button
            type="button"
            data-bs-target="#languagesCarousel"
            data-bs-slide-to="0"
            class="active hide-small"
          ></button>
          <button
            type="button"
            data-bs-target="#languagesCarousel"
            data-bs-slide-to="1"
            class="hide-small"
          ></button>
          <button
            type="button"
            data-bs-target="#languagesCarousel"
            data-bs-slide-to="2"
            class="hide-small"
          ></button>
          <button
            type="button"
            data-bs-target="#languagesCarousel"
            data-bs-slide-to="3"
            class="hide-small"
          ></button>
          <button
            type="button"
            data-bs-target="#languagesCarousel"
            data-bs-slide-to="4"
            class="hide-small"
          ></button>
          <button
            type="button"
            data-bs-target="#languagesCarousel"
            data-bs-slide-to="5"
            class="hide-small"
          ></button>
          <button
            type="button"
